#include <stdio.h>

int if_perfect(int number) {
    int counter, i, temp;
    counter = 1;
    temp = number;
    i = 2;
    while (i * i <= number) {
        if (number % i == 0)
            counter += i;
        if (number % i == 0 && number / i != i) {
            counter += number / i;
        }
        i += 1;
    }
    if (counter == temp) {
        return 1;
    }
    return 0;

}

int main() {
    int m, n, i, idx, counter;
    counter = 0;

    int result[10];
    idx = 0;
    for (idx = 0; idx <= 9; ++idx) {
        result[idx] = 0;
    }

    scanf("%d%d", &m, &n);
    if (m == 1) {
        m += 1;
    }
    idx = 0;
    for (i = m; i <= n; ++i) {
        if (if_perfect(i)) {
            result[idx] = i;
            idx += 1;
            counter += 1;
        }
    }
    printf("%d\n", counter);
    idx = 0;
    for (idx = 0; idx <= 9; ++idx) {
        if (result[idx] != 0) {
            printf("%d ", result[idx]);
        }
    }
    return 0;
}
